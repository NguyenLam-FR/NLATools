import { IgxColumnComponent } from '../columns/column.component';
/**
 * This enumeration is used to configure whether the pinning possition is set before or after
 * the target. DropPosition.None is acting like DropPosition.AfterDropTarget.
 */
export declare enum DropPosition {
    BeforeDropTarget = 0,
    AfterDropTarget = 1,
    None = 2
}
/**
 * @hidden
 * @internal
 */
export declare class IgxColumnMovingService {
    private _icon;
    private _column;
    cancelDrop: boolean;
    isColumnMoving: boolean;
    get column(): IgxColumnComponent;
    set column(val: IgxColumnComponent);
    get icon(): any;
    set icon(val: any);
}
